<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

<profiles>
        <profile>
            <id>live</id>
            <repositories>
                <repository>
                    <id>max-maven-hosted-test</id>
                    <name>max-maven-hosted-test</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-hosted-test</url>
                </repository>
                <repository>
                    <id>max-maven-central</id>
                    <name>max-maven-central</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-central</url>
                </repository>
            </repositories>
        </profile>
        <profile>
            <id>beta</id>
            <repositories>
                <repository>
                    <id>max-maven-hosted-test</id>
                    <name>max-maven-hosted-test</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-hosted-test</url>
                </repository>
                <repository>
                    <id>max-maven-central</id>
                    <name>max-maven-central</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-central</url>
                </repository>
            </repositories>
        </profile>
        <profile>
            <id>dev</id>
            <repositories>
                <repository>
                    <id>max-maven-hosted-test</id>
                    <name>max-maven-hosted-test</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-hosted-test</url>
                </repository>
                <repository>
                    <id>max-maven-central</id>
                    <name>max-maven-central</name>
                    <url>http://webtech-lin-staging.srv.media.net:8081/repository/max-maven-central</url>
                </repository>
            </repositories>
          </profile>
            <profile>
            <id>nexus</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>http://repository.media.net/nexus/content/repositories/central</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
                <repository>
                    <id>third party</id>
                    <url>http://repository.media.net/nexus/content/repositories/thirdparty</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>http://repository.media.net/nexus/content/repositories/central</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>

    </profiles>
      <mirrors>
        <mirror>
            <id>nexus</id>
            <mirrorOf>*</mirrorOf>
            <url>http://repository.media.net/nexus/content/groups/public</url>
        </mirror>
    </mirrors>

    <servers>
          <server>
            <id>medianet</id>
            <username>deployer</username>
            <password>deployer</password>
          </server>
          <server>
            <id>max-maven-hosted-test</id>
            <username>admin</username>
            <password>admin123</password>
          </server>

  </servers>
      <activeProfiles>
        <activeProfile>nexus</activeProfile>
    </activeProfiles>

</settings>
